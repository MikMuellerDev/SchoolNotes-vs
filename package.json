{
	"publisher": "mikmueller",
	"repository": "https://github.com/MikMuellerDev/SchoolNotes-vs",
	"icon": "assets/schoolnotes.png",
	"name": "schoolnotes",
	"displayName": "SchoolNotes",
	"description": "Companion extension for SchoolNotes",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:md2pdf.compileInteractive",
		"onCommand:md2pdf.compileWithDate"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md2pdf.compileInteractive",
				"title": "convert to pdf (interactive)"
			},
			{
				"command": "md2pdf.compileWithDate",
				"title": "convert to pdf with date"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == markdown",
					"command": "md2pdf.compileInteractive",
					"group": "test"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "md2pdf.compileWithDate",
					"group": "test"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == markdown",
					"command": "md2pdf.compileInteractive",
					"group": "test"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "md2pdf.compileWithDate",
					"group": "test"
				}
			]
		},
		"scripts": {
			"lint": "eslint .",
			"pretest": "npm run lint",
			"test": "node ./test/runTest.js"
		},
		"devDependencies": {
			"@types/vscode": "^1.59.0",
			"@types/glob": "^7.1.3",
			"@types/mocha": "^8.2.2",
			"@types/node": "14.x",
			"eslint": "^7.27.0",
			"glob": "^7.1.7",
			"mocha": "^8.4.0",
			"typescript": "^4.3.2",
			"vscode-test": "^1.5.2"
		}
	}
}
